{
  "jsdoc": {
    "debug": 5,
    "sort": "kind",
    "hierarchy": true
  },
  "src": ["./src/models/**/*.js"],
  "dest": "./docs/developer/core/dist",
  "assets": {
    "/img": ["./docs/developer/core/assets/img/*.*"]
  },
  "template": {
    "path": "zebra",
    "options": {
      "title": {
        "label": "Core Dcomentation",
        "href": "/base/"
      },
      "logo": {
        "dark": "img/asls.logo.blue.only.png",
        "light": "img/asls.logo.blue.only.png"
      },
      "sidebar": {
        "enabled": true,
        "outline": "tree",
        "collapsed": false,
        "toolbar": true,
        "itemsFolded": false,
        "itemsOverflow": "crop",
        "badges": true,
        "search": true,
        "animations": true
      },
      "symbols": {
        "autoLink": true,
        "params": "list",
        "enums": "list",
        "props": "list",
        "meta": false
      },
      "contentView": {
        "bookmarks": "h1,h2,h3",
        "faLibs": "all"
      },
      "navbar": {
        "enabled": true,
        "dark": false,
        "animations": true,
        "menu": [
          {
            "label": "User Guide",
            "iconClass": "fas fa-book",
            "href": "https://studio.asls.timekadel.com/introduction/",
            "target": "_blank"
          },
          {
            "label": "Demo",
            "iconClass": "fas fa-laptop",
            "href": "https://demo.studio.asls.timekadel.com"
          },
          {
            "label": "GitHub",
            "iconClass": "fab fa-github",
            "href": "https://github.com/ASLS-org/studio",
            "target": "_blank"
          }
        ]
      }
    }
  }
}
